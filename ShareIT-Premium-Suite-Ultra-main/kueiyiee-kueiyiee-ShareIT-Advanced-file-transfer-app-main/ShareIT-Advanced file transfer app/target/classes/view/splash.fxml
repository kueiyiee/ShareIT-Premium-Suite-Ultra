<?xml version="1.0" encoding="UTF-8"?>
<VBox xmlns="http://javafx.com/javafx/17.0.2" 
      xmlns:fx="http://javafx.com/fxml/1"
      alignment="CENTER" 
      style="-fx-background-color: #0a0a0a;">
    
    <ImageView fitWidth="120" fitHeight="120">
        <Image url="@/images/shareit_logo.png" />
